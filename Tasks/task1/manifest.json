{
    "name": "Color Picker",
    "description": "add your own color to the background",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "color.html"
      },
    "content_scripts":[{
        "matches": ["<all_urls>"],
        "js":["color.js"],
        "run_at":"document_end"
    }]

    }